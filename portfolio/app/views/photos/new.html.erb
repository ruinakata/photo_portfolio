<h1>New Photo</h1>

<%= form_for :photo, url: photos_path do |f| %>
  <p>
    <%= f.label :title %><br>
    <%= f.text_field :title %>
  </p>
 
  <p>
    <%= f.label :description %><br>
    <%= f.text_area :description %>
  </p>

  <p>
    <%= f.label :tags %><br>
    <%= f.text_area :tags %>
  </p>

  <p>
    <%= f.label :photo_url %><br>
    <%= f.text_area :photo_url %>
  </p>
 
  <p>
    <%= f.submit %>
  </p>
<% end %>